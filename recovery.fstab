/boot         emmc     boot                     flags=backup=1;display=boot;flashimg=1
/cache        ext4     /dev/block/mmcblk0p2     flags=backup=1;display=cache;flashimg=1
/data         ext4     /dev/block/mmcblk0p3     flags=backup=1;display=data;flashimg=1
/misc         emmc     misc                     flags=backup=1;display=misc;flashimg=1
/recovery     emmc     recovery                 flags=backup=1;display=recovery;flashimg=1
/sdcard       vfat     /dev/block/mmcblk0p4     flags=display=sdcard
/system       ext4     /dev/block/mmcblk0p6     flags=backup=1;display=system;flashimg=1
/custom       ext4     /dev/block/mmcblk0p7     flags=backup=1;display=custom;flashimg=1
